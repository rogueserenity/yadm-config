[user]
	name = Jay Burgess
	email = jay@rogueserenity.com
	signingkey = ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAIK0wxfvgbEafn/OhaHoWfswCoInD5+0AZ4PbqYOloXCC
[core]
	excludesFile = ~/.config/git/ignore
	autocrlf = input
[commit]
	gpgsign = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = main
[gpg]
	format = ssh
[url "git@github.com:"]
	insteadOf = https://github.com/
[fetch]
	prune = true
[pull]
	rebase = true
[gpg "ssh"]
	program = /Applications/1Password.app/Contents/MacOS/op-ssh-sign
	allowedSignersFile = /Users/jay/.ssh/allowed_signers
